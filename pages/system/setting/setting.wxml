<view class="settingDiv">
    <view class="group">
        云存储
        <text class="h">用于数据备份及还原</text>
    </view>
    <van-cell-group title="阿里云 OSS">
        <van-field model:value="{{ OSS_ALI_ID }}" label="Key ID" placeholder="请前往阿里云控制台查看" />
        <van-field model:value="{{ OSS_ALI_SECRET }}" label="Key Secret" placeholder="请前往阿里云控制台查看" />
    </van-cell-group>
    <van-cell-group title="数据备份">
        <van-notice-bar left-icon="info-o" scrollable="{{false}}" text="OSS参数为空时备份操作将忽略" />
        <!-- <van-cell center title="个人密码本" class="switch">
            <van-switch size="22px"  data-key="OSS_BACKUP_PASSWORD" checked="{{ OSS_BACKUP_PASSWORD }}" 
                active-color="{{color}}" bind:change="onSwitchChange" />
        </van-cell> -->
        <van-cell wx:for="{{ dataBeans }}" wx:key="index" center title="{{ item.name }}" class="switch">
            <van-switch size="22px"  data-key="{{ item.configKey }}" data-index="{{ index }}" checked="{{ item.value }}" 
                active-color="{{color}}" bind:change="onSwitchChange" />
        </van-cell>
    </van-cell-group>
</view>

<view class="flexBtn">
    <van-button icon="success" type="primary" round bind:tap="onSave"></van-button>
</view>

<wxs module="M">
    module.exports.key = function(bean){
        return true
    }
</wxs>